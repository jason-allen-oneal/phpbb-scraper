# Base URL of the phpBB forum
BASE_URL=https://example.com/forum/

# User Agent for requests
USER_AGENT=Mozilla/5.0 (X11; Linux x86_64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/113.0.0.0 Safari/537.36

# Optional: Cookies (if you want to bypass login)
DF_COOKIES=

# Storage configuration
OUTPUT_MODE=file  # Options: file, db
OUTPUT_DIR=output

# Database configuration (only needed if OUTPUT_MODE=db)
DB_TYPE=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_USER=root
DB_PASS=
DB_NAME=phpbb_scraper
